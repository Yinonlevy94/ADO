<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>

    <!-- Enable NuGet packing for a Web project -->
    <IsPackable>true</IsPackable>

    <!-- Optional but recommended metadata -->
    <PackageId>hello-world-api</PackageId>
    <Authors>YourName</Authors>
    <Company>YourCompany</Company>
    <Description>Hello World API sample</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/Yinonlevy94/ADO</RepositoryUrl>
  </PropertyGroup>

  <!-- existing ItemGroup/PackageReferences remain as-is -->
</Project>
